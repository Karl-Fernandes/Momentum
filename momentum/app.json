{
  "expo": {
    "name": "momentum",
    "slug": "momentum",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.krf0821.momentum",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krf0821.momentum"
    },
    "android": {
      "package": "com.krf0821.momentum",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
    {
      "android": {
        "compileSdkVersion": 34,
        "targetSdkVersion": 34,
        "buildToolsVersion": "34.0.0",
        "kotlinVersion": "1.9.0"
      }
    }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "04aaa9e8-e046-48b5-a089-d6a4650e39f7"
      },
      "googleWebClientId": "443859253397-j8r0bfv4lv1kptt5gpbfh5pr58e0df71.apps.googleusercontent.com",
      "googleIosClientId": "YOUR_IOS_CLIENT_ID.apps.googleusercontent.com",
      "googleAndroidClientId": "443859253397-t9nlfh326arq2aid4evjr81i2j72u5rg.apps.googleusercontent.com",
      "router": {
        "origin": false
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "krf0821"
  }
}
